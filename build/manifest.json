{
  "manifest_version": 2,
  "name": "vk.com music downloader",
  "description": "Adds [Download] link in every audio block.",
  "version": "1.1",
  "web_accessible_resources": ["vk_inject.js", "libs.js"],

  "content_scripts": [
    {
      "matches": ["*://*.vk.com/*"],
      "js": ["script.js"],
      "css": ["vk_style.css"]
    }
  ]
}
